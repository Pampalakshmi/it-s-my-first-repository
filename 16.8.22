#include <stdio.h>

int main()
{
   int i,n,arr[n],num,j,f;
   printf("enter the n value");
   scanf("%d",&n);
   printf("enter the elements into array\n");
   for(i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   for(i=n-1;i>=0;i--)
   {
       num=arr[i];
       f=0;
       for(j=2;j<num;j++)
       {
           if(num%j==0)
           {
               f=1;
               break;
           }
       }
       if(f==0)
       printf("%d is prime\n",num);
   }
    return 0;
}
