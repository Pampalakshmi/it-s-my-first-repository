#include <stdio.h>

int main()
{
   int i,n,arr[n],count=0;
   printf("enter the n value");
   scanf("%d",&n);
   printf("enter the elements into array\n");
   for(i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
       if(arr[i]>=10)
       {
       count++;
       }
   }
   printf("%d",count);
    return 0;
}
